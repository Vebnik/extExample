{
  "manifest_version": 2,
  "name": "Name",
  "description": "description",
  "version": "0.0.1",
  "icons": {"128": "./media/iconsDefault.png"},
  "background": {
    "persistent": true,
    "scripts": ["./src/bg.bundle.js"]
  },
  "browser_action": {
    "default_icon": "./media/iconsDefault.png"
  },
  "content_scripts": [
    {
      "js": [
        "./src/index.bundle.js"
      ],
      "matches": [
        "*://crm2.skyeng.ru/*"
      ]
    }
  ],
  "permissions": [
    "*://crm2.skyeng.ru/*",
    "cookies",
    "tabs",
    "activeTab",
    "storage",
    "notifications"
  ]
}